package com.walmart.services;

import com.walmart.exceptions.StockException;
import com.walmart.pojo.Product;

public interface ProductServiceInterface {

	public int addProduct(Product p)throws StockException;
	public int editProdcut(int qty,double price, int pid)throws BuisnessException;
	public int removeProduct(int pid)throws BuisnessException;
	public Product getProdcutById(int id)throws BuisnessException;
	public List<Product> getAll()throws BuisnessException;
}
